extends layout

block content
  .w3-card-4 
    header.w3-container.w3-blue
      h1= title 
    .w3-container
      ul.w3-ul.w3-card-4
        li
          b Nome:
          span= compositor.nome 
        li
          b Bio:
          span= compositor.bio
        li
          b Data de Nascimento:
          span= compositor.dataNasc
        li
          b Data de Falecimento:
          span= compositor.dataObito
        li
          b Periodo:
          span= compositor.periodo
    
    footer.w3-container.w3-blue
      address Gerado por EngWeb 2024 em #{data} - 
        | 
        a(href='/compositores') [Retornar]
                    
               